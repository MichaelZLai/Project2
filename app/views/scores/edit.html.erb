<div class="headbar">
<h2>Whoops</h2>
<p>Did I miss something?</p>
</div>

<h3>Edit Score</h3>
<div id="form">
<%= form_for [@workout,@score] do |f| %>

<p><%= f.label :class_time %></p>
<p><%= f.text_field :workout_time %></p>

<p><%= f.label :score %></p>
<p><%= f.text_field :workout_score %></p>

<p><%= f.label :notes %></p>
<p><%= f.text_area :notes %></p>

<p><%= f.submit "Submit Your Score!" %></p>
<% end %>

<%= link_to "Back", workout_path(@workout) %>
</div>
