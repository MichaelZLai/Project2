<div class="headbar">
<h2>Whoops</h2>
<p>Did I miss something?</p>
</div>

<h3>Edit Score</h3>
<%= form_for [@workout,@score] do |f| %>

<p><%= f.label :class_time %>
<%= f.text_field :workout_time %></p>

<p><%= f.label :score %>
<%= f.text_field :workout_score %></p>

<p><%= f.label :notes %>
<%= f.text_area :notes %></p>

<%= f.submit "Submit Your Score!" %>
<% end %>

<%= link_to "Back", workout_path(@workout) %>
