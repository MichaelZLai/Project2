<div class="headbar">
<h2>Edit Workout</h2>
<p>Easy there Pls.</p>
</div>

<%= form_for @workout do |f| %>

<p><%= f.label :workout_date %>
<%= f.date_field :workout_date, value: f.object.workout_date %></p>

<p><%= f.label :workout_name %>
<%= f.text_field :name %></p>

<p><%= f.label :workout %>
<%= f.text_area :wod %></p>

<%= f.submit "Edit WOD!" %>

<% end %>

<%= link_to "Back", workout_path(@workout) %>
